 #include<stdio.h>
int main()
{
    int n,rem,rev=0,temp;
    printf("enter the number\n");
    scanf("%d",&n);
    temp=n;
    while(temp!='\0')
    {
    rev=rev*10;
    rev=rev+temp%10;
    temp=temp/10;
    }
    printf("reverse number:%d\n",rev);
    if(rev==n)
        printf("%d is palindrome number\n",n);
    else
        printf("not palindrome number");
}
