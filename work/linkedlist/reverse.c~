struct_reverse(struct node **phead_ref)

{
struct node *p=NULL,*q=*phead,*r;
while(q!=NULL)
{
r=q->next;
q->next=p;
p=q;
q=r;

}
*phead_ref=p;

}
int main()
{
    /* Start with the empty list */
    struct Node* head = NULL;
   
     push(&head, 20);
     push(&head, 4);
     push(&head, 15); 
     push(&head, 85);      
     
     printf("Given linked list\n");
     printList(head);    
     reverse(&head);                      
     printf("\nReversed Linked list \n");
     printList(head);    
     getchar();
}
